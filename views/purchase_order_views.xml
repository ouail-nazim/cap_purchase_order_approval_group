<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="custom_intern_company_purchase_order_form_view" model="ir.ui.view">
      <field name="name">Custom intern company purchase order form view</field>
      <field name="model">purchase.order</field>
      <field name="inherit_id" ref="purchase.purchase_order_form"/>
      <field name="arch" type="xml">
        <button name="button_approve" position="attributes">
          <attribute name="groups"></attribute>
        </button>
        <field name="partner_ref" position="after">
          <!-- <field name="higher_validation_id"/> -->
          <field name="division_id"/>
          <field name="next_group"/>
        </field>
      </field>
    </record>
  </data>
</odoo>
